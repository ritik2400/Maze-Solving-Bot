void rev_corr_factor(char S)
{
  switch(S)
  {
    case 'r':
    rev_corr_fact = -50;
    break;
    case 'l':
    rev_corr_fact = -50;
    break;
    case 'a':
    rev_corr_fact = -50;
    break;
    case 's':
    rev_corr_fact = -50;
    break;
    case 'R':
    rev_corr_fact = 0;
    break;
    case 'L':
    rev_corr_fact = 0;
    break;
    case 'S':
    rev_corr_fact = 0;
    break;
    case 'U':
    rev_corr_fact = 0;
    break;
  }
}
